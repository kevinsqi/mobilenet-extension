{
  "manifest_version": 2,
  "name": "MobileNet",
  "author": "Kevin Qi",
  "version": "0.1.0",
  "description": "MobileNet extension",
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches": [
        "http://*/*",
        "https://*/*"
      ],
      "js": ["vendor/tfjs_1.0.1.js", "vendor/mobilenet_v1.0.0.js", "content.js"],
      "run_at": "document_end"
    }
  ],
  "permissions": ["contextMenus"]
}
